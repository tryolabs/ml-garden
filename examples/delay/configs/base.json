{
    "pipeline": {
        "name": "XGBoostTrainingPipeline",
        "description": "Training pipeline for XGBoost models.",
        "parameters": {
            "save_data_path": "delay.pkl",
            "target": "Delay",
            "task": "classification",
            "columns_to_ignore_for_training": [
                "FlightID",
                "FlightNumber"
            ]
        },
        "steps": [
            {
                "step_type": "GenerateStep",
                "parameters": {
                    "train_path": "examples/delay/data/train.csv",
                    "predict_path": "examples/delay/data/test.csv"
                }
            },
            {
                "step_type": "TabularSplitStep",
                "parameters": {
                    "train_percentage": 0.6,
                    "validation_percentage": 0.2,
                    "test_percentage": 0.2
                }
            },
            {
                "step_type": "CleanStep"
            },
            {
                "step_type": "CalculateFeaturesStep",
                "parameters": {
                    "datetime_columns": [
                        "ScheduledDeparture",
                        "ScheduledArrival"
                    ],
                    "features": [
                        "minute",
                        "hour",
                        "weekday",
                        "month"
                    ]
                }
            },
            {
                "step_type": "EncodeStep",
                "parameters": {
                    "feature_encoders": {
                        "Origin": {
                            "encoder": "OrdinalEncoder"
                        },
                        "Destination": {
                            "encoder": "OrdinalEncoder"
                        }
                    }
                }
            },
            {
                "step_type": "ModelStep",
                "parameters": {
                    "model_class": "XGBoostClassifier"
                }
            },
            {
                "step_type": "CalculateMetricsStep"
            },
            {
                "step_type": "ExplainerDashboardStep",
                "parameters": {
                    "enable_step": false
                }
            }
        ]
    }
}